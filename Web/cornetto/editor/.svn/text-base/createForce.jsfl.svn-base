// caching variablesvar doc = fl.getDocumentDOM();var lib = fl.getDocumentDOM().library;var items = lib.items;var item;var curvedWall;var forceElement;lib.selectNone();// Element propertiesvar elementBaseClass;//linkageClassNamevar elementClass;//linkageClassNamevar elementType;//itemTypevar elementX;var elementY;var elementRotation;var elementZIndex;var elementName;var elementRotation;// counter to help to see when we need to close the tagvar tileCounter = 0;// get all the elements from the libraryfor (var i = 0; i < items.length; i++){	item = items[i];		if(item.name.indexOf("ForceElement") >= 0)	{		curvedWall = item;	}	}var counter = 0 ; var tempArray = new Array() ; for(var i = 0; i<items.length; i++){	item = items[i];	if(item.name.indexOf("ForceItem_") >= 0)	{		var ind_ = item.name.indexOf("_") ; 		tempArray.push( item.name.substr( ind_+1 , item.name.length - ind_ ) ) ; 	}	}if( tempArray.length > 0 ){	counter = Number(tempArray[ tempArray.length-1 ]) + 1  ; }lib.duplicateItem(curvedWall.name);var newCurve = lib.getSelectedItems()[0];lib.renameItem("ForceItem_"+counter);newCurve.linkageExportForAS = true;newCurve.linkageExportInFirstFrame = true;newCurve.linkageIdentifier = "ForceItem_"+counter;newCurve.linkageClassName = "ForceItem_"+counter;lib.moveToFolder("force", newCurve.name);lib.updateItem(newCurve.name);lib.addItemToDocument({x:300,y:250},newCurve.name);lib.selectNone();
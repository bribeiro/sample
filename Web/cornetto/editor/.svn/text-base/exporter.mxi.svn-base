<macromedia-extension name="Assets Exporter" version="1.0" type="flashpanel">      <author name="Bruno Ribeiro" />      <products>          <product name="Flash" version="9" primary="true" />      </products>      <description>          <![CDATA[Export assets]]>      </description>      <ui-access>          <![CDATA[Window > Other Panels > Exporter]]>      </ui-access>      <files>          <file source="createForce.jsfl" destination="$Flash\WindowSWF" />        <file source="createCurve.jsfl" destination="$Flash\WindowSWF" />        <file source="createMoving.jsfl" destination="$Flash\WindowSWF" />        <file source="exporter.jsfl" destination="$Flash\WindowSWF" />        <file name="exporter.swf" destination="$Flash\WindowSWF" />      </files>  </macromedia-extension>
// caching variablesvar doc = fl.getDocumentDOM();var lib = fl.getDocumentDOM().library;var items = lib.items;var item;var moving;lib.selectNone();// Element propertiesvar elementBaseClass;//linkageClassNamevar elementClass;//linkageClassNamevar elementType;//itemTypevar elementX;var elementY;var elementRotation;var elementZIndex;var elementName;var elementRotation;lib.selectNone();// counter to help to see when we need to close the tagvar tileCounter = 0;// get all the elements from the libraryfor (var i = 0; i < items.length; i++){	item = items[i];		if(item.name.indexOf("MovingElement") >= 0)	{		moving = item;	}}var counter = 0 ; var tempArray = new Array() ; for(var i = 0; i<items.length; i++){	item = items[i];	if(item.name.indexOf("MovingItem_") >= 0)	{		var ind_ = item.name.indexOf("_") ; 		tempArray.push( item.name.substr( ind_+1 , item.name.length - ind_ ) ) ; 	}	}if( tempArray.length > 0 ){	counter = Number(tempArray[ tempArray.length-1 ]) + 1  ; }lib.duplicateItem(moving.name);var newMoving = lib.getSelectedItems()[0];lib.renameItem("MovingItem_"+counter);newMoving.linkageExportForAS = true;newMoving.linkageExportInFirstFrame = true;newMoving.linkageIdentifier = "MovingItem_"+counter;newMoving.linkageClassName = "MovingItem_"+counter;lib.moveToFolder("moving", newMoving.name);lib.updateItem(newMoving.name);lib.addItemToDocument({x:300,y:250},newMoving.name);lib.selectNone();